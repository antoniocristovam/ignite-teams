{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@theme/*": [
        "./src/theme/*",
      ],
      "@utils/*": [
        "./src/utils/*",
      ],
      "@assets/*": [
        "./src/assets/*",
      ],
      "@router/*": [
        "./src/router/*",
      ],
      "@screens/*": [
        "./src/screens/*",
      ],
      "@storage/*": [
        "./src/storage/*",
      ],
      "@components/*": [
        "./src/components/*",
      ],
    }
  }
}
